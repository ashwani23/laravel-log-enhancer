{
    "name": "ashwanigoyal/laravel-log-enhancer",
    "description": "This is a small package to enhance the logs provided by laravel",
    "type": "package",
    "authors": [
        {
            "name": "Ashwani Goyal",
            "email": "ashwanigoyal@outlook.in"
        }
    ],
    "minimum-stability": "beta",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*",
        "monolog/monolog": "1.23.*"
    },
    "license": "MIT",
    "autoload": {
       "psr-4": {
            "Ashwanigoyal\\LaravelLogEnhancer\\": "src/"
        }
    },
    "extra": {
    	"laravel": {
		"providers": [
			"Ashwanigoyal\\LaravelLogEnhancer\\LaravelLogEnhancerServiceProvider"
		]
	}
    }
}
